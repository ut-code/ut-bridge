{
  "name": "server",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development bun run --env-file=../.env --hot index.ts",
    "check": "bun tsc",
    "build": "bun run check && bun run :build",
    ":build": "bun build index.ts --target bun --outfile target/index.js",
    "prisma": "bun run --env-file=../.env bunx prisma",
    "prepare": "prisma generate",
    "start": "bun run ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.946.0",
    "@aws-sdk/s3-presigned-post": "^3.946.0",
    "@aws-sdk/s3-request-presigner": "^3.946.0",
    "@hono/zod-validator": "^0.7.5",
    "@prisma/client": "^6.19.0",
    "common": "workspace:common",
    "devalue": "^5.5.0",
    "firebase-admin": "^13.6.0",
    "hono": "^4.10.7",
    "web-push": "^3.6.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "@types/web-push": "^3.6.4",
    "prisma": "^6.19.0",
    "typescript": "^5.9.3"
  },
  "trustedDependencies": [
    "@prisma/client"
  ]
}
