    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "daisyui": "^5.0.0-beta.8",
    "postcss": "^8",
    "tailwindcss": "^4.0.6",
    "typescript": "^5",
    "server": "workspace:server",
    "common": "workspace:common",
    "@inlang/paraglide-js": "2.0.0-beta.26",
    "@inlang/cli": "^3.0.0"
